GARMETS COMPANY HACKERRANK

import java.io.*;
import java.util.*;

public class Solution {
  public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int c=0;
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=s.nextInt();
        }
       
     for(int i=0;i<n;i++){
    double sq=(int)Math.sqrt(a[i]);
    if(sq*sq==a[i]){
        c++;
    }
}
    System.out.println(c);
}
}

    

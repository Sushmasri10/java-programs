REMAINDER OF NO.OF DIGITS

import java.io.*;
import java.util.*;
import java.lang.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int r=(int)Math.log10(n)+1;
        int d=n%r;
        System.out.println(d);
        
                
    }
}
